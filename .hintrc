{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "no-inline-styles": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "-webkit-text-size-adjust",
          "-moz-appearance"
        ]
      }
    ]
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not edge <= 114",
    "not ios_saf <= 16.5",
    "not safari <= 16.5"
  ]
}